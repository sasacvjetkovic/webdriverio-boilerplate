pipeline {
    agent any 
    stages {
        stage('Stage 1') {
            steps {
                echo 'Hello world!'
            }
        }
        stage('Stage 2') {
            steps {
                sh 'apt-get update'
                sh 'apt-get -y upgrade'
                sh 'apt-get dist-upgrade'
                sh 'apt-get -y install build-essential'
                sh 'apt install npm'
                sh 'npm -v'
                sh 'ls'
            }
        }
    }
}
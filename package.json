{
  "name": "clean-wdio-ts-v2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "android": "wdio ./config/wdio.conf.android.js",
    "ios": "wdio ./config/wdio.conf.ios.js",
    "ios_tag": "wdio ./config/wdio.conf.ios.js --cucumberOpts.tagExpression=\"$TAG\"",
    "ios_optional": "wdio ./config/wdio.conf.ios.js --cucumberOpts.tagExpression=${TAG=@login}",
    "ios2": "wdio ./config/wdio.conf.ios.js --cucumberOpts.tagExpression=",
    "ios_saucelabs": "wdio ./config/wdio.conf.ios.saucelabs.js --cucumberOpts.tagExpression=\"$TAG\"",
    "e2e-custom": "npm run $SCRIPT -- --cucumberOpts.tagExpression=\"$TAG\"",
    "allure-report": "allure generate report/allure-results --clean && allure open",
    "report:history": "mkdir -p report/allure-report/history allure-results/history && cp -a report/allure-report/history report/allure-results/",
    "allure": "allure serve report/allure-results",
    "report.ci": "allure generate report/allure-results --clean -o allure-report"
  },
  "author": "Sasa Cvjetkovic",
  "license": "ISC",
  "devDependencies": {
    "@types/cucumber": "^6.0.1",
    "@types/node": "^14.14.14",
    "@types/webdriverio": "^5.0.0",
    "@wdio/allure-reporter": "^6.11.0",
    "@wdio/appium-service": "^6.10.11",
    "@wdio/cli": "^6.10.11",
    "@wdio/cucumber-framework": "^6.10.11",
    "@wdio/local-runner": "^6.10.13",
    "@wdio/sauce-service": "^6.11.0",
    "@wdio/spec-reporter": "^6.10.6",
    "@wdio/sync": "^6.10.11",
    "allure-commandline": "^2.13.8",
    "appium": "^1.20.0-beta.0",
    "appium-webdriveragent": "^2.32.2",
    "cucumber": "^7.0.0-rc.0",
    "react-native": "^0.63.4",
    "typescript": "^4.1.3",
    "webdriverio": "^6.12.0"
  },
  "dependencies": {
    "carthage": "^1.0.6",
    "ts-node": "^9.1.1"
  }
}
